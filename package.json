{
  "name": "numeric",
  "publisher": "nealbkn",
  "displayName": "numeric",
  "icon": "images/numeric.png",
  "description": "Hover to see numeric translations",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "license": "Apache-2.0",
  "repository": {
    "url": "https://github.com/nbbeeken/numeric"
  },
  "browser": "dist/numeric.js",
  "main": "dist/numeric.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "node ./.github/scripts/esbuild.js --minify",
    "compile": "node ./node_modules/typescript/bin/tsc",
    "watch": "node ./.github/scripts/esbuild.js --watch --sourcemap",
    "build": "node ./.github/scripts/esbuild.js --sourcemap",
    "package": "vsce package",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p -w . --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "release": "standard-version -i changelog.md",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "17.x",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vscode/test-electron": "^2.1.4",
    "@vscode/test-web": "^0.0.24",
    "chai": "^4.3.6",
    "esbuild": "^0.14.43",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.0.0",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.3",
    "vsce": "^2.9.1"
  }
}
